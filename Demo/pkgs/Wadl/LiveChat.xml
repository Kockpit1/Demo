  <resources siena:authenticationProviderHref="#PowerAppAuth" base="https://india-001.azure-apim.net/apim/livechat">
    <resource path="/{connectionId}/agents">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="GET" id="ListAgents" visibility="important" siena:isDeprecated="false">
        <doc title="List agents&#xA;This operation returns a list of all the agents in your LiveChat account." />
        <request />
        <response>
          <representation mediaType="application/json" element="service:ListAgents_Response" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/agents">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="POST" id="CreateAgent" visibility="important" siena:isDeprecated="false">
        <doc title="Create a new agent&#xA;This operation is used to add a new agent to your LiveChat account." />
        <request>
          <representation mediaType="application/json">
            <param style="plain" name="login" path="/login" type="xs:string" required="true" />
            <param style="plain" name="name" path="/name" type="xs:string" required="true" />
            <param style="plain" name="login_status" path="/login_status" type="xs:string">
              <option value="accepting chats" />
              <option value="not accepting chats" />
            </param>
            <param style="plain" name="job_title" path="/job_title" type="xs:string" />
            <param style="plain" name="permission" path="/permission" type="xs:string">
              <option value="administrator" />
              <option value="normal" />
            </param>
            <param style="plain" name="password" path="/password" type="xs:string" />
            <param style="plain" name="max_chats_count" path="/max_chats_count" type="xs:string" />
          </representation>
        </request>
        <response>
          <representation mediaType="application/json" element="service:CreateAgent_Response" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/agents/{login}">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="login" type="xs:string" required="true">
        <doc title="The email of the agent." />
      </param>
      <method siena:requiresAuthentication="true" name="DELETE" id="DeleteAgent" visibility="advanced" siena:isDeprecated="false">
        <doc title="Remove an agent&#xA;This operation is used to delete an agent from your LiveChat account." />
        <request />
        <response>
          <representation mediaType="application/json" element="service:DeleteAgent_Response" />
        </response>
      </method>
    </resource>
